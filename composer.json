{
    "name": "monstrex/ave",
    "description": "Ave Admin Panel - A lightweight admin package for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "admin", "panel", "ave"],
    "authors": [
        {
            "name": "MonstreX",
            "email": "fido6080net@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/routing": "^12.0",
        "illuminate/validation": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/view": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "orchestra/testbench": "^10.0",
        "laravel/pint": "^1.24"
    },
    "autoload": {
        "psr-4": {
            "Monstrex\\Ave\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Monstrex\\Ave\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Monstrex\\Ave\\Providers\\AveServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit tests/Unit/ --testdox"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
